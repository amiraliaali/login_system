{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build main",
            "type": "shell",
            "command": "bazel build -c dbg //:main",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            },
        },
        {
            "label": "run main",
            "type": "shell",
            "command": "./bazel-bin/main",
            "args": [
                "${workspaceFolder}/car_data_set.csv",
            ],
            "group": "none",
            "presentation": {
                "reveal": "always",
                "panel": "shared"
            }
        },
    ]
}